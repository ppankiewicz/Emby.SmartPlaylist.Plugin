{
    "compilerOptions": {
        "sourceMap": true,
        "noImplicitAny": false,
        "module": "commonjs",
        "target": "esnext",
        "lib": ["es2015", "es2017", "dom"],
        "removeComments": true,
        "allowSyntheticDefaultImports": true,
        "jsx": "react",
        "allowJs": true,
        "baseUrl": ".",
        "paths": {
            "~/*": ["./src/*"]
        }
    }
}
